<div class="col-md-6 col-md-offset-1 col-xs-10 col-xs-offset-1 output-container" ng-controller="outcomeFormController">
    <div class="output-header">
        <div class="output-header-title">
            搜尋結果
        </div>
    </div>
    <div class="output-outcome">
        <div class="output-detail" ng-repeat="answer in answers| startFrom:currentPage*pageSize | limitTo:pageSize">
            <div class="output-detail-title">
                <a ng-href="{{answer.link}}">{{answer.title}}</a>
            </div>
        </div>
    </div>
    <div class="page-container">
        <button ng-disabled="currentPage == 0" ng-click="currentPage=currentPage-1">
            <
        </button>
        {{currentPage+1}}/{{numberOfPages()}}
        <button ng-disabled="currentPage >= answers.length/pageSize - 1" ng-click="currentPage=currentPage+1">
            >
        </button>
    </div>
</div>
<div class="col-md-6 col-md-offset-1 col-xs-10 col-xs-offset-1 loader-box">
    <div class="loading">Loading</div>
    <div class="loader"></div>
</div>
